<!-- Build assets for library1.  Note that all paths are defined
     relative to ${multi.dir} since we don't know what the current
     directory is in which Ant is running. -->

<project name="library1.assets">
    <!-- For convenience, set up the name of the project directory -->
    <property name="multi.library1.dir" location="${multi.dir}/library1"/>

    <!-- Define SWC exported by this library. -->
    <property name="multi.library1.swc" location="${multi.library1.dir}/${build.compc.dir}/library1.swc"/>
</project>
