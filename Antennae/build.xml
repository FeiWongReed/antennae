<project name="Antennae" default="build" basedir=".">
    <property name="root.dir" location="." />
    <import file="build-imports.xml" />

    <filelist id="children.filelist" dir=".">
        <file name="examples"/>
    </filelist>

    <target name="init">
        <echoAndRun target="init"/>
    </target>

    <target name="build">
        <echoAndRun target="build"/>
    </target>

    <target name="test">
        <echoAndRun target="test"/>
    </target>

    <target name="stage">
        <echoAndRun target="stage"/>
    </target>

    <target name="dist">
        <echoAndRun target="dist"/>
    </target>

    <target name="deploy">
        <echoAndRun target="deploy"/>
    </target>

    <target name="clean">
        <echoAndRun target="clean"/>
    </target>
</project>
